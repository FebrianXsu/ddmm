{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "includePattern": ".js$",
      "exclude": ["src/utility/logger.js"]
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": "true",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "add_style_path": ["./styles/custom.css", "./styles/pretty.css"]
    }
  }
}